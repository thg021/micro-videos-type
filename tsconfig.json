{
    "compilerOptions": {
        "incremental": true,
        "target": "es2017",
        "experimentalDecorators": true,
        "module": "commonjs",
        "baseUrl": "./src",
        "paths": {
            "#seedwork/*": ["./@seedwork/*"],
            "#category/*": ["./category/*"]
        },
        "allowJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "skipLibCheck": true
    },
    "include": ["src/"]
}
